{
	"name": "Lei Light",
	"include": "./light_plus.json",
	"settings": [
		{
			"name": "settings",
			"settings": {
				"background": "#F9F2E5",
        "caret": "#000000",
				"foreground": "#000000",
				"invisibles": "#dddddd",
				"lineHighlight": "#E0E0E0",
				"selection": "#cccccc",
				"rangeHighlight": "#3392FF3B",
				"selectionHighlight": "#3392FF3B",
				"inactiveSelection": "#3392FF3B",
				"wordHighlight": "#3392FF3B",
				"wordHighlightStrong": "#3392FF3B",
				"findMatchHighlight": "#3392FF3B",
				"currentFindMatchHighlight": "#3392FF3B",
				"findRangeHighlight": "#3392FF3B",
				"activeLinkForeground": "#a71d5d",
				"hoverHighlight": "#3392FF3B",
				"referenceHighlight": "#3392FF3B"
			}
		},
    {
			"scope": "comment",
			"settings": {
				"foreground": "#777777"
			}
		},
    {
      "name": "Keyword",
			"scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.operator.expression",
        "variable.language",
        "constant.language",
        "meta.preprocessor",
        "storage",
        "storage.type",
        "storage.modifier",

        "string.comment.buffered.block.jade",
				"string.quoted.jade",
				"string.interpolated.jade",

				"string.unquoted.plain.in.yaml",
				"string.unquoted.plain.out.yaml",
				"string.unquoted.block.yaml",
				"string.quoted.single.yaml",

				"string.quoted.double.xml",
				"string.quoted.single.xml",
				"string.unquoted.cdata.xml",

				"string.quoted.double.html",
				"string.quoted.single.html",
				"string.unquoted.html",

				"string.quoted.single.handlebars",
				"string.quoted.double.handlebars",

        "punctuation.definition.template-expression.begin.ts",
				"punctuation.definition.template-expression.end.ts"
      ],
			"settings": {
				"foreground": "#0808D1",
        "fontStyle": "normal"
			}
		},
    {
			"scope": [
				"constant.numeric",
        "markup.inserted",
        "meta.preprocessor.numeric",
        "keyword.other.unit",
        "constant.sha.git-rebase",
        "constant.language"
			],
			"settings": {
				"foreground": "#BF0099"
			}
		},
    {
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",

				"support.constant.handlebars"
			],
			"settings": {
				"foreground": "#268BD2"
			}
		},
    {
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.class",

				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",

				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",

				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy",

        "meta.return.type",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",

				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
    {
			"name": "XML/HTML tags",
			"scope": [
        "punctuation.definition.tag",
        "entity.name.tag"
      ],
			"settings": {
				"foreground": "#0077aa"
			}
		},
    {
			"scope": [
        "entity.name.tag"
      ],
			"settings": {
        "fontStyle": "bold"
			}
		},
    {
			"scope": [
        "entity.other.attribute-name"
      ],
			"settings": {
				"foreground": "#BF0099"
			}
		}
	]
}
